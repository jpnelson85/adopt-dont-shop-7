<h1>All Shelters</h1>

<% @shelters.each do |shelter| %>
    <h3>Name: <%= shelter.name %></h3>
    <% end %>
<br>
<section id="shelters">
    <h2>Shelters with Pending Applications</h2>
    <ul>
    <% @pending_app_shelters.each do |shelter| %>
    <li> <%= shelter.name %> </li>
    <% end %>
    </ul>
</section>

